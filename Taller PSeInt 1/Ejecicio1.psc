//Realizar un programa el cual solicite su nombre y apellidos.

Proceso Ejercicio1

	Definir nombre Como Caracter;
	Definir apellidos Como Caracter;
	
	//Posibilidad 1: Todo en una sola variable
	Escribir "Ingrese su nombre completo: ";
	Leer nombre;
	
	//Posibilidad 2: Una variable para el nombre y otra para los apellidos
	Escribir "Ingrese su primer nombre: ";
	Leer nombre;
	Escribir "Ingrese su apellido/apellidos";
	Leer apellidos;
	
FinProceso